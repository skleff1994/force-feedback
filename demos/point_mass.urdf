<?xml version="1.0" ?>
<!-- URDF of the KUKA iiwa 14 robot generated by robcogen and extended manually -->
<!-- Added interias and other properties by hand.                               -->
<robot name="PointMass">

  <link name="contact">
    <!-- Modeling the contact as a tennis ball. -->
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0.0"/>
      <mass value="0.057"/>
      <inertia ixx="0.0000354" ixy="0.0" ixz="0.0" iyy="0.0000354" iyz="0.0" izz="0.0000354"/>
    </inertial>

    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://config/sphere.obj" scale="0.065 0.065 0.065"/>/>
      </geometry>
      <material name="grey">
        <color rgba="0.8 0.8 0.8 1.0"/>
      </material>
    </visual>

    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://config/sphere.obj" scale="0.065 0.065 0.065"/>/>
      </geometry>
      <material name="grey">
        <color rgba="0.8 0.8 0.8 1.0"/>
      </material>
    </collision>

    <!-- Bullet specific paramters -->
    <contact>
      <lateral_friction value="1.0"/>
      <restitution value="0.5"/>
    </contact>
  </link>

</robot>